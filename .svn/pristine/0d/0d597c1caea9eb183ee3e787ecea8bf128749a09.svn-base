package com.xyy.springmvc.mybatis.beans;

import java.util.Date;

/**
 * 
* @ClassName: Ruleitem 
* @Description: 基础规则项
* @author klj
* @date 2016年11月7日 下午3:34:43 
*
 */
public class Ruleitem {
	private String riEvalOperator;// 比较运算符
	private String riValue1;// 比较值1
	private String riVvalue2;// 比较值2
	private String riLink;// 链接符
	private Date riMtnDate;// 维护日期
	private String riOprId;// 维护操作员
	private String riRuleNo;// 规则编号
	private Double riSeqNo;// 序号
	private String riDataName;// 信息点ID
	private String riParameters;// 参数
	public String getRiEvalOperator() {
		return riEvalOperator;
	}
	public void setRiEvalOperator(String riEvalOperator) {
		this.riEvalOperator = riEvalOperator;
	}
	public String getRiValue1() {
		return riValue1;
	}
	public void setRiValue1(String riValue1) {
		this.riValue1 = riValue1;
	}
	public String getrVvalue2() {
		return riVvalue2;
	}
	public void setrVvalue2(String riVvalue2) {
		this.riVvalue2 = riVvalue2;
	}
	public String getRiLink() {
		return riLink;
	}
	public void setRiLink(String riLink) {
		this.riLink = riLink;
	}
	public Date getRiMtnDate() {
		return riMtnDate;
	}
	public void setRiMtnDate(Date riMtnDate) {
		this.riMtnDate = riMtnDate;
	}
	public String getRiOprId() {
		return riOprId;
	}
	public void setRiOprId(String riOprId) {
		this.riOprId = riOprId;
	}
	public String getRiRuleNo() {
		return riRuleNo;
	}
	public void setRiRuleNo(String riRuleNo) {
		this.riRuleNo = riRuleNo;
	}
	public Double getRiSeqNo() {
		return riSeqNo;
	}
	public void setRiSeqNo(Double riSeqNo) {
		this.riSeqNo = riSeqNo;
	}
	public String getRiDataName() {
		return riDataName;
	}
	public void setRiDataName(String riDataName) {
		this.riDataName = riDataName;
	}
	public String getRiParameters() {
		return riParameters;
	}
	public void setRiParameters(String riParameters) {
		this.riParameters = riParameters;
	}
	@Override
	public String toString() {
		return "Ruleitem [riEvalOperator=" + riEvalOperator + ", riValue1="
				+ riValue1 + ", riVvalue2=" + riVvalue2 + ", riLink=" + riLink
				+ ", riMtnDate=" + riMtnDate + ", riOprId=" + riOprId
				+ ", riRuleNo=" + riRuleNo + ", riSeqNo=" + riSeqNo
				+ ", riDataName=" + riDataName + ", riParameters="
				+ riParameters + "]";
	}
	
}
